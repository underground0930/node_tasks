{
  "name": "frontend_tasks",
  "version": "3.5.0",
  "description": "node.js frontend_tasks",
  "main": "index.js",
  "private": true,
  "scripts": {
    "apiServer": "docker run -p 4000:80 -v ${PWD}/htdocs_api:/var/www/html -d php:apache",
    "prod": "run-s build del-dummy",
    "build": "run-p webpack:prod tasks:prod",
    "dev": "run-p webpack:dev tasks:dev",
    "dev-param": "cross-env PARAM1=\"100\" PARAM2=\"200\" PARAM3=\"300\" run-p webpack:dev tasks:dev",
    "tasks:prod": "cross-env NODE_ENV=\"production\" node ./tasks/index.js",
    "tasks:dev": "cross-env NODE_ENV=\"development\" node ./tasks/index.js",
    "webpack:prod": "cross-env NODE_ENV=\"production\" webpack",
    "webpack:dev": "cross-env NODE_ENV=\"development\" webpack",
    "del-dummy": "rm -R htdocs/assets/json/_dummy htdocs/assets/img/_dummy htdocs/assets/movie/_dummy",
    "optim": "npm run optim:png && npm run optim:jpg",
    "optim:png": "find . -name '*.png' | xargs pngquant --ext .png --force -v",
    "optim:jpg": "find . -name '*.jpg' | xargs jpegoptim --strip-all --max=90",
    "lint:ejs": "ejslint 'src/**/*.{ejs,html}'",
    "lint": "eslint 'src/assets/js/**/*' --fix --ext .ts,.tsx"
  },
  "browserslist": [
    "last 2 version"
  ],
  "eslintIgnore": [
    "/.eslintrc.js"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/autoprefixer": "^10.2.0",
    "@types/browser-sync": "^2.26.2",
    "@types/fs-extra": "^9.0.13",
    "@types/jquery": "^3.5.6",
    "@types/js-yaml": "^4.0.3",
    "@types/jump.js": "^1.0.4",
    "@types/react": "^17.0.27",
    "@types/react-dom": "^17.0.9",
    "@types/sass": "^1.16.1",
    "@types/swiper": "^5.4.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@wordpress/env": "^4.1.1",
    "autoprefixer": "^10.3.7",
    "browser-sync": "^2.27.5",
    "chokidar": "^3.5.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.4.0",
    "del": "^6.0.0",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.7",
    "ejs-lint": "^1.2.1",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.3.0",
    "express": "^4.17.1",
    "fs-extra": "^10.0.0",
    "glob": "^7.2.0",
    "http-proxy-middleware": "^2.0.1",
    "js-beautify": "^1.14.0",
    "js-yaml": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.9",
    "prettier": "^2.4.1",
    "sass": "^1.42.1",
    "source-map-loader": "^3.0.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.3",
    "webpack": "^5.58.1",
    "webpack-cli": "^4.9.0"
  },
  "dependencies": {
    "axios": "^0.25.0",
    "current-device": "^0.10.2",
    "gsap": "^3.8.0",
    "jquery": "^3.6.0",
    "jump.js": "^1.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "swiper": "^7.0.8"
  }
}

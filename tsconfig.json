{
  "compilerOptions": {
    "target": "ES5",
    "module": "es2015",
    "lib": ["es2020", "DOM"],
    "strict": true,
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {
      "*": ["./src/@types/*"]
    },
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/assets/js/**/*","src/@types/*"],
  "exclude": ["node_modules",]
}

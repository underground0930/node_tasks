{
  "name": "node_tasks",
  "version": "2.4.0",
  "description": "node.js実装環境",
  "main": "index.js",
  "private": true,
  "scripts": {
    "build": "run-p webpack:prod tasks:prod",
    "dev": "run-p webpack:dev tasks:dev",
    "tasks:prod": "node ./tasks/index.js",
    "tasks:dev": "cross-env NODE_ENV=\"dev\" node ./tasks/index.js",
    "webpack:prod": "webpack",
    "webpack:dev": "cross-env NODE_ENV=\"dev\" webpack",
    "optim": "npm run optim:png && npm run optim:jpg",
    "optim:png": "find . -name '*.png' | xargs pngquant --ext .png --force -v",
    "optim:jpg": "find . -name '*.jpg' | xargs jpegoptim --strip-all --max=90",
    "wp-start": "wp-env start",
    "wp-stop": "wp-env stop",
    "lint:ejs": "ejslint 'src/**/*.{ejs,html}'",
    "lint": "eslint 'src/assets/js/**/*' --fix --ext .ts,.tsx"
  },
  "browserslist": [
    "last 2 version",
    "IE 11"
  ],
  "eslintIgnore": [
    "/.eslintrc.js"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/autoprefixer": "^10.2.0",
    "@types/browser-sync": "^2.26.1",
    "@types/fs-extra": "^9.0.7",
    "@types/jquery": "^3.5.5",
    "@types/js-yaml": "^4.0.0",
    "@types/jump.js": "^1.0.3",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/sass": "^1.16.0",
    "@types/swiper": "^5.4.2",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "@wordpress/env": "^3.0.2",
    "autoprefixer": "^9.8.6",
    "browser-sync": "^2.26.14",
    "chokidar": "^3.5.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.2",
    "del": "^6.0.0",
    "ejs": "^3.1.6",
    "ejs-lint": "^1.1.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.0.0",
    "fs-extra": "^9.1.0",
    "glob": "^7.1.6",
    "js-beautify": "^1.13.13",
    "js-yaml": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.6",
    "prettier": "^2.2.1",
    "sass": "^1.32.8",
    "source-map-loader": "^2.0.1",
    "ts-loader": "^8.0.17",
    "typescript": "^4.1.5",
    "webpack": "^5.24.0",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "current-device": "^0.10.2",
    "gsap": "^3.6.0",
    "intersection-observer": "^0.12.0",
    "jquery": "^3.5.1",
    "jump.js": "^1.0.2",
    "objectFitPolyfill": "^2.3.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "swiper": "^6.5.7"
  }
}

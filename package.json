{
  "name": "node_tasks",
  "version": "1.7.1",
  "description": "node.js実装環境",
  "main": "index.js",
  "scripts": {
    "build": "run-p webpack:prod tasks:prod",
    "dev": "run-p webpack:dev tasks:dev",
    "tasks:prod": "node ./tasks/index.js",
    "tasks:dev": "cross-env NODE_ENV=\"dev\" node ./tasks/index.js",
    "webpack:prod": "webpack",
    "webpack:dev": "cross-env NODE_ENV=\"dev\" webpack",
    "optim": "npm run optim:png && npm run optim:jpg",
    "optim:png": "find . -name '*.png' | xargs pngquant --ext .png --force -v",
    "optim:jpg": "find . -name '*.jpg' | xargs jpegoptim --strip-all --max=90",
    "lint:ejs": "ejslint 'src/**/*.{ejs,html}'"
  },
  "browserslist": [
    "last 2 version",
    "IE 11"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-object-assign": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/register": "^7.12.1",
    "@babel/runtime-corejs3": "^7.12.5",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.1.0",
    "browser-sync": "^2.26.14",
    "chokidar": "^3.4.3",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.2",
    "del": "^5.1.0",
    "ejs": "^3.1.6",
    "ejs-lint": "^1.1.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "fs-extra": "^9.1.0",
    "glob": "^7.1.6",
    "js-yaml": "^3.14.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.35",
    "sass": "^1.32.8",
    "vue-loader": "^15.9.6",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.24.0",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "intersection-observer": "^0.11.0",
    "objectFitPolyfill": "^2.3.5",
    "swiper": "^5.4.5",
    "vue": "^2.6.12"
  }
}

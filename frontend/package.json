{
  "name": "frontend_tasks",
  "version": "3.6.0",
  "description": "node.js frontend_tasks",
  "main": "index.js",
  "private": true,
  "scripts": {
    "apiServer": "docker run -p 4000:80 -v ${PWD}/htdocs_api:/var/www/html -d php:apache",
    "prod": "run-s build del-dummy",
    "build": "run-p webpack:prod tasks:prod",
    "dev": "run-p webpack:dev tasks:dev",
    "dev-param": "cross-env PARAM1=\"100\" PARAM2=\"200\" PARAM3=\"300\" run-p webpack:dev tasks:dev",
    "tasks:prod": "cross-env NODE_ENV=\"production\" node ./tasks/index.js",
    "tasks:dev": "cross-env NODE_ENV=\"development\" node ./tasks/index.js",
    "webpack:prod": "cross-env NODE_ENV=\"production\" webpack",
    "webpack:dev": "cross-env NODE_ENV=\"development\" webpack",
    "del-dummy": "rm -R htdocs/assets/json/_dummy htdocs/assets/img/_dummy htdocs/assets/movie/_dummy",
    "optim": "npm run optim:png && npm run optim:jpg",
    "optim:png": "find . -name '*.png' | xargs pngquant --ext .png --force -v",
    "optim:jpg": "find . -name '*.jpg' | xargs jpegoptim --strip-all --max=90",
    "lint:ejs": "ejslint 'src/**/*.{ejs,html}'",
    "lint": "eslint 'src/assets/js/**/*' --fix --ext .ts,.tsx"
  },
  "browserslist": [
    "last 2 version"
  ],
  "eslintIgnore": [
    "/.eslintrc.js"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/autoprefixer": "^10.2.0",
    "@types/browser-sync": "^2.26.3",
    "@types/fs-extra": "^9.0.13",
    "@types/jquery": "^3.5.14",
    "@types/js-yaml": "^4.0.5",
    "@types/jump.js": "^1.0.4",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@types/sass": "^1.43.1",
    "@types/swiper": "^5.4.3",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "autoprefixer": "^10.4.7",
    "browser-sync": "^2.27.10",
    "chokidar": "^3.5.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "del": "^6.1.1",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "ejs-lint": "^1.2.2",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "express": "^4.18.1",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "http-proxy-middleware": "^2.0.6",
    "js-beautify": "^1.14.3",
    "js-yaml": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "sass": "^1.52.1",
    "source-map-loader": "^3.0.1",
    "ts-loader": "^9.3.0",
    "typescript": "^4.7.2",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "current-device": "^0.10.2",
    "gsap": "^3.10.4",
    "jquery": "^3.6.0",
    "jump.js": "^1.0.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "swiper": "^8.1.6"
  }
}

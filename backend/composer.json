{
    "require-dev": {
        "squizlabs/php_codesniffer": "3.6"
    },
    "scripts": {
      "phpcs": "phpcs --standard=PSR2 --extensions=php --ignore=./wp-content/themes/template/cache/* ./wp-content/themes/template",
      "phpreport": "phpcs --report=summary --ignore=./wp-content/themes/template/cache/* ./wp-content/themes/template",
      "phpcbf": "phpcbf --standard=PSR2 --extensions=php --ignore=./wp-content/themes/template/cache/* ./wp-content/themes/template"
    }
}

